{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Regino",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.regino",
				"match": "\\b(if|while|for|return|block|when|case|of|func|break|continue)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.regino",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.regino",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.rgn"
}