{
    "name": "vscode-regino",
    "displayName": "vscode-regino",
    "description": "Provides Language support for Regino.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "regino",
            "aliases": ["Regino", "regino"],
            "extensions": [".rgn"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "regino",
            "scopeName": "source.rgn",
            "path": "./syntaxes/regino.tmLanguage.json"
        }]
    }
}